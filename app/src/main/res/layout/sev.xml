<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <TableLayout
                android:id="@+id/table"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_below="@+id/login_desc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                >

                <TableRow>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="4"
                        android:background="@drawable/border"
                        android:gravity="center_horizontal"
                        android:padding="10sp"
                        android:text="Total Refer"
                        android:textColor="@color/black"
                        android:textSize="12sp" />
                    <TableLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"

                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="4"
                        android:stretchColumns="0"
                        android:gravity="center">


                        <TableRow

                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="4"

                            >

                            <TextView
                                android:textSize="15dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="Main Team"
                                android:textAlignment="center"
                                android:layout_column="0"
                                android:background="@drawable/border"
                                android:textStyle="normal"
                                android:gravity="center"
                                />

                            <TextView
                                android:visibility="gone"

                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="B"
                                android:textAlignment="center"

                                android:layout_column="1"
                                android:background="@drawable/border"
                                android:gravity="center"
                                android:textStyle="normal"
                                />

                            <TextView
                                android:visibility="gone"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="C"
                                android:textAlignment="center"
                                android:background="@drawable/border"
                                android:gravity="center"
                                android:textStyle="normal"
                                android:layout_column="2"
                                />
                        </TableRow>



                    </TableLayout>






                </TableRow>
                <TableRow>


                    <TextView
                        android:background="@drawable/border"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:text="Total Members"
                        android:padding="10sp"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />



                    <TableLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"

                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="4"
                        android:stretchColumns="0"
                        android:gravity="center">


                        <TableRow

                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"

                            >

                            <TextView
                                android:id="@+id/teamA"
                                android:textSize="15dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="0"
                                android:layout_column="0"
                                android:background="@drawable/border"
                                android:textStyle="normal"
                                android:gravity="center"
                                />

                            <TextView
                                android:visibility="gone"
                                android:id="@+id/teamB"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="0"


                                android:layout_column="1"
                                android:background="@drawable/border"
                                android:gravity="center"
                                android:textStyle="normal"
                                />

                            <TextView
                                android:visibility="gone"
                                android:id="@+id/teamC"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="0"
                                android:background="@drawable/border"
                                android:gravity="center"
                                android:textStyle="normal"
                                android:layout_column="2"
                                />
                        </TableRow>



                    </TableLayout>

                </TableRow>




                <TableRow
                    android:visibility="gone">

                    <TextView
                        android:background="@drawable/border"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:text="Login ID "
                        android:padding="10sp"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />



                    <TableLayout
                        xmlns:android="http://schemas.android.com/apk/res/android"

                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_weight="4"
                        android:stretchColumns="0,1,2"
                        android:gravity="center">


                        <TableRow

                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"

                            >

                            <TextView

                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="0"
                                android:textAlignment="center"
                                android:layout_column="0"
                                android:background="@drawable/border"
                                android:textStyle="normal"
                                android:gravity="center"
                                />

                            <TextView

                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="0"
                                android:textAlignment="center"

                                android:layout_column="1"
                                android:background="@drawable/border"
                                android:gravity="center"
                                android:textStyle="normal"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="0"
                                android:textAlignment="center"
                                android:background="@drawable/border"
                                android:gravity="center"
                                android:textStyle="normal"
                                android:layout_column="2"
                                />
                        </TableRow>



                    </TableLayout>

                </TableRow>
                <TableRow
                    android:visibility="gone">


                    <TextView
                        android:background="@drawable/border"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:text="Leaders Club\nBonus"
                        android:padding="10sp"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />



                    <TextView
                        android:background="@drawable/border"
                        android:text="@string/work"
                        android:id="@+id/royal"
                        android:layout_width="0dp"
                        android:layout_height="50dp"

                        android:padding="10sp"
                        android:textSize="12dp"
                        android:textAllCaps="false"
                        android:textColor="#000"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />

                </TableRow>
                <TableRow>


                    <TextView
                        android:visibility="gone"
                        android:background="@drawable/border"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:text="Monthly Income"
                        android:padding="10sp"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />



                    <TextView
                        android:visibility="gone"
                        android:background="@drawable/border"
                        android:text="@string/work"
                        android:id="@+id/monthly"
                        android:layout_width="0dp"
                        android:layout_height="50dp"

                        android:padding="10sp"
                        android:textSize="12dp"
                        android:textAllCaps="false"
                        android:textColor="#000"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />

                </TableRow>
                <TableRow>

                    <TextView
                        android:background="@drawable/border"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:visibility="gone"
                        android:text="Rating   \nBonus"
                        android:padding="10sp"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />



                    <TextView
                        android:background="@drawable/border"
                        android:text="@string/work"
                        android:layout_width="0dp"
                        android:id="@+id/match"
                        android:layout_height="50dp"
                        android:visibility="gone"
                        android:padding="10sp"
                        android:textSize="12dp"
                        android:textAllCaps="false"
                        android:textColor="#000"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />

                </TableRow>
                <TableRow>

                    <TextView
                        android:visibility="gone"
                        android:background="@drawable/border"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:text="Matching    \nBonus"
                        android:padding="10sp"
                        android:textColor="@color/black"
                        android:textSize="12sp"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />



                    <TextView
                        android:background="@drawable/border"
                        android:text="0"
                        android:layout_width="0dp"
                        android:visibility="gone"
                        android:layout_height="50dp"

                        android:padding="10sp"
                        android:textSize="12dp"
                        android:textAllCaps="false"
                        android:textColor="#000"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />

                </TableRow>
                <TableRow

                    >
                    <TextView
                        android:background="@drawable/border"
                        android:text="Account Create Date : "

                        android:layout_width="0dp"
                        android:layout_height="50dp"

                        android:padding="10sp"
                        android:textSize="15dp"
                        android:textAllCaps="false"
                        android:textColor="#000"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />
                    <TextView
                        android:background="@drawable/border"
                        android:text="11-2-2021"
                        android:id="@+id/logindate"
                        android:layout_width="0dp"
                        android:layout_height="50dp"

                        android:padding="10sp"
                        android:textSize="15dp"
                        android:textAllCaps="false"
                        android:textColor="#000"
                        android:layout_weight="4"
                        android:gravity="center_horizontal"
                        />
                </TableRow>


            </TableLayout>

        </RelativeLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>